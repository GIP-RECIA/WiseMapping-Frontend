{
    "presets": [
        [
            "@babel/preset-env",
            {
                "corejs": "3.15",
                "useBuiltIns": "usage",
                "targets": {
                    "esmodules": true
                }
            }
        ]
    ],
    "plugins": ["@babel/plugin-transform-modules-commonjs"],
    "sourceType": "unambiguous"
}
