{
    "env": {
        "browser": true,
        "commonjs": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "max-len": [0, 200],
        "no-underscore-dangle": "off",
        "no-restricted-syntax": "off",
        "guard-for-in": "off",
        "no-plusplus": "off"
    },
    "plugins": ["only-warn"]

}